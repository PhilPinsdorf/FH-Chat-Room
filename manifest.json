{
    "name": "FH Aachen Chat Room",
    "description": "Building a Chat Room Extension for the FH Aachen, because we are bored!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage"],
    "background": {
        "service_worker": "background/background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "/images/icon16.png",
            "32": "/images/icon32.png",
            "48": "/images/icon48.png",
            "128": "/images/icon128.png"
        }
    },
    "icons": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    }
}